{
  "name": "schema-evolution-monitor",
  "version": "1.0.0",
  "description": "Monitor schema changes and their impact on downstream systems",
  "main": "backend/src/index.js",
  "scripts": {
    "start": "node backend/src/index.js",
    "start:dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon backend/src/index.js",
    "client:dev": "cd frontend && vite",
    "build": "cd frontend && vite build",
    "test": "jest",
    "init-db": "node backend/src/db/init.js",
    "postinstall": "npm run build"
  },
  "keywords": ["schema", "monitoring", "deployment", "streaming"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vitejs/plugin-react": "^4.4.1",
    "axios": "^1.9.0",
    "better-sqlite3": "^11.9.1",
    "body-parser": "^2.2.0",
    "bootstrap": "^5.3.6",
    "cors": "^2.8.5",
    "d3": "^7.9.0",
    "express": "^5.1.0",
    "pipelinepulse": "^1.0.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.5.3",
    "sqlite3": "^5.1.7",
    "streamsynth": "^0.2.0",
    "vite": "^6.3.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.0"
  }
}